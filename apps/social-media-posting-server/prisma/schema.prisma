datasource db {
  provider = "postgresql"
  url      = env("DB_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Post {
  agentName      String?
  agentPhone     String?
  createdAt      DateTime @default(now())
  description    String?
  id             String   @id @default(cuid())
  propertyImages Json?
  updatedAt      DateTime @updatedAt
}
